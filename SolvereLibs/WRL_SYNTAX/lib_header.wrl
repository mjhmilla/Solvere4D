DEF TS TimeSensor {
cycleInterval $TIME$
loop TRUE
enabled FALSE
}

Background { skyColor $RGB$ }

DEF DefaultLight DirectionalLight {
	ambientIntensity 0.5
	color	1 1 1
	direction $DL_DIRECTION$
	intensity 1
	on $DL_ON$
}

DEF DefaultNavInfo NavigationInfo {
	headlight $HEADLIGHT$
	type ["WALK" "ANY"]
}
